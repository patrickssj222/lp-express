{
  "name": "lp-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "dotenv": "^8.0.0",
    "express": "~4.17.1",
    "express-pdf": "^1.2.2",
    "html-pdf": "^2.2.0",
    "html2canvas": "^1.0.0-rc.3",
    "http-errors": "~1.7.3",
    "jade": "~1.11.0",
    "jspdf": "^1.5.3",
    "morgan": "~1.9.1",
    "mysql": "^2.17.1",
    "phantom": "^6.2.0",
    "react-to-print": "^2.2.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
  },
  "engines": {
    "node": "8.10.0"
  }
}
